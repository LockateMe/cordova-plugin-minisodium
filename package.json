{
  "name": "cordova-plugin-minisodium",
  "version": "1.0.3",
  "description": "A minimal binding to the libsodium library, as a plugin for Cordova applications on iOS and Android",
  "main": "index.js",
  "cordova": {
    "id": "cordova-plugin-curve25519",
    "platforms": [
      "ios",
      "android"
    ]
  },
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
