    "ecosystem:cordova",
    "cordova",
    "cordova-ios",
    "cordova-android",
    "libsodium",
    "sodium",
    "NaCl",
    "curve25519",
    "ed25519",
    "X25519",
    "scrypt",
    "blake2b",
    "ecdh",
    "eddsa",
    "secretbox",
    "box",
    "sealed-box",
    "hash",
    "crypto",
    "cryptography"
  ],
  "devDependencies": {
    "cordova-plugin-test-framework": ">=1.0.0",
    "grunt": "^1.0.1",
    "grunt-jsvalidate": "^0.2.2"
  },
  "author": "Ahmad Benmrad",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LockateMe/cordova-plugin-minisodium/issues"
